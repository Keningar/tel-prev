	float: left;
	min-width: 80px;
	height: 20px;
	padding: 4px;
}
.item_info_usuario{
    border-left: 1px solid transparent;
    border-right: 1px solid #BBBBBB;
    color: #000000;
	padding-right: 5px;
    display: block;
    font-size: 12px;
    position: relative;
    text-align: center;
    z-index: 1000;
}
