body {
	margin: 0 !important;
	padding: 0 !important;
	font-family: "Lucida Sans Unicode","Lucida Grande",Verdana,Arial,Helvetica,sans-serif;
	font-size: 11px;
	color: #333333;
	width:1429px !important;
}

h1, h2, h3 {
	margin: 0;
	font-family: "Lucida Sans Unicode","Lucida Grande",Verdana,Arial,Helvetica,sans-serif;
	font-weight: normal;
	color: #2D2D2D;
}

h1 {
	font-size: 36px;
}

h2 {
	font-size: 24px;
}

h3 {
	font-size: 16px;
}

p, ol, ul {
	margin-top: 0;
	line-height: 200%;
	text-align: justify;
}

a {
	/*color: #B40000;*/
	text-decoration: none;
}

a:hover {
	text-decoration: none;
}

a img {
	border: none;
}

img.border {
}

img.alignleft {
	float: left;
}

img.alignright {
	float: right;
}

img.aligncenter {
	margin: 0 auto;
}
.rounded-corners{
  border-radius: 5px !important;
  moz-border-radius: 5px !important;
  webkit-border-radius: 5px !important;
}

hr {
	display: none;
}

.container {
	margin: 0 auto;
}

.inputs{
margin-left: 50px;

}

.labels{
    margin-right: 50px;
}

.clearfix {
	clear: both;
}

/* Header */

#header {
	height: 122px;
}

/* Logo */

#logo {
	float: left;
	width: 375px;
	height: 162px;
	padding-left: 40px;
}

#logo h1 {
	padding-top: 50px;
        font-size:3em;
}

#logo p {
	padding-left: 2px;
	font: 14px "Lucida Sans Unicode","Lucida Grande",Verdana,Arial,Helvetica,sans-serif;
	color: #919191;
}

#logo a {
	text-decoration: none;
	color: #2D2D2D;
}

/* topBars */
#topBars{
    background-color: #FFFFFF;
    font-size: 20px;
    height: 28px;
    opacity: 1;
    top: 0;
    z-index: 990;
	right:0;
	color:white;
}

#search_login input{
    color: #C0C0C0;
    height: 20px;
    margin-top: 3px;
    margin-right: 4px;
    padding: 0 10px 0 6px;
    width: 15em;
    border-color: #C0C0C0 #D9D9D9 #D9D9D9;
    border-right: 1px solid #D9D9D9;
    border-style: solid;
    border-width: 1px;
}
#search_login{
	float:right;
}

/* menu modulos*/
#middleBar{
    background: none repeat scroll 0 0 #F1F1F1;
    border-bottom: 1px solid #C1CCD6;
    border-top: 1px solid #C1CCD6;
    box-shadow: 0 0 3px #D7D7D7;
    padding:10px 10px 10px 20px;
    min-height:4em;
    
    -moz-border-bottom-colors: none;
    -moz-border-image: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    -moz-box-sizing: content-box !important;   
}
#logo_modulo{
	float:left;
	padding-right:1em;
}
#logo_sit{
    height: 4em;
	float:left;
	text-align:center;
	margin-right: 15px;
	margin-top:-4px;
}
#menu_modulo{
	margin-top: -1px;
	margin-left: 150px;
}
#menu_modulo ul {
	margin: 0 !important;
	list-style: none !important;
	line-height: normal !important;
	font: 14px "Lucida Sans Unicode","Lucida Grande",Verdana,Arial,Helvetica,sans-serif !important;
}
#menu_modulo li.active{
    background: -moz-linear-gradient(center top , #EEEEEE, #E0E0E0) repeat scroll 0 0 transparent !important;
    border-color: #BBBBBB !important;
    box-shadow: 0 1px 5px #CCCCCC inset !important;
    z-index: 2 !important;
}
#menu_modulo li.active a p{
	color: #B40000;
	font-weight:bold;
}
#menu_modulo li:hover{
    background: -moz-linear-gradient(center top , #EEEEEE, #E0E0E0) repeat scroll 0 0 transparent;
    border-color: #BBBBBB;
    box-shadow: 0 1px 5px #CCCCCC inset;
    z-index: 2;
}
#menu_modulo li{
    border-color: #D9D9D9;
    border-style: solid;
    border-width: 1px 2px 2px 1px;
    float: left;
    height: 2.8em;
    margin-right: 0.7em;
    padding: 2px;
    text-align: center;
    vertical-align: middle;
    min-width: 6em;
    -moz-box-sizing: content-box !important;
}
#menu_modulo li a img.img_menu{
	float:left;
	margin: 3px;
}
#menu_modulo li a p{
	color: #333333;
    float: right;
    font-size: 12px;
    margin-top: 1.7em;
    padding-right: 0.3em;
}
#nombre_modulo {
    color: #666666;
    font-size: 1.15em;
    margin-top: -8px;
    text-align: right;
}
#menu_modulos{
	float:left;
	min-width: 25em;
}

/* Menu modulos */


#menu_modulos ul {
	margin: 0;
	list-style: none;
	line-height: normal;
	font: 14px "Lucida Sans Unicode","Lucida Grande",Verdana,Arial,Helvetica,sans-serif;
}

#menu_modulos li {
	float: left;
	min-width: 80px;
	height: 20px;
	padding: 4px;  
    -moz-box-sizing: content-box !important;      
}

#menu_modulos a {
	text-decoration: none;
	color: white;
}

#menu_modulos a:hover {
	text-decoration: none;
}
#menu_modulos li.active a {
	color: #FFFFFF;
}
.item_menu_modulo{
    border-left: 1px solid transparent;
    border-right: 1px solid transparent;
    display: block;
    position: relative;
    z-index: 1000;
	text-align:center;
	color:#000000;
	font-size:13px;
}

/* info usuario */
#info_usuario{
	float:right;
	min-width: 17em;
}

#info_usuario ul {
	margin: 0;
	list-style: none;
