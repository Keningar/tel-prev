--UPDATE

UPDATE DB_COMERCIAL.ADMI_PRODUCTO_CARACTERISTICA
SET ESTADO                        = 'Eliminado'
WHERE ID_PRODUCTO_CARACTERISITICA =
  (SELECT ID_PRODUCTO_CARACTERISITICA
  FROM DB_COMERCIAL.ADMI_PRODUCTO_CARACTERISTICA
  WHERE PRODUCTO_ID     = 1040
  AND CARACTERISTICA_ID =
    (SELECT ID_CARACTERISTICA
    FROM DB_COMERCIAL.ADMI_CARACTERISTICA
    WHERE DESCRIPCION_CARACTERISTICA = 'TIPO LICENCIAMIENTO SERVICE'
    AND ESTADO                       = 'Activo'
    )
  );

----

UPDATE DB_COMERCIAL.INFO_SERVICIO_PROD_CARACT SET VALOR = 'Licencia VEEAM BACKUP x vm' WHERE ID_SERVICIO_PROD_CARACT IN 
(SELECT 
SPC.ID_SERVICIO_PROD_CARACT 
FROM
DB_COMERCIAL.INFO_SERVICIO S,
DB_COMERCIAL.INFO_SERVICIO_PROD_CARACT SPC,
DB_COMERCIAL.ADMI_PRODUCTO_CARACTERISTICA PC,
DB_COMERCIAL.ADMI_CARACTERISTICA AC
WHERE
S.ID_SERVICIO = SPC.SERVICIO_ID AND
SPC.PRODUCTO_CARACTERISITICA_ID = PC.ID_PRODUCTO_CARACTERISITICA AND
PC.CARACTERISTICA_ID = AC.ID_CARACTERISTICA AND
SPC.ESTADO = 'Activo'  AND
AC.DESCRIPCION_CARACTERISTICA = 'TIPO LICENCIAMIENTO SERVICE' AND
SPC.VALOR = 'Licencia VEEAM BACKUP x  vm');

UPDATE DB_COMERCIAL.INFO_SERVICIO_PROD_CARACT
SET VALOR                      = 'Licencia VEEAM Replicacion x vm'
WHERE ID_SERVICIO_PROD_CARACT IN
  (SELECT SPC.ID_SERVICIO_PROD_CARACT
  FROM 
    DB_COMERCIAL.INFO_SERVICIO S,
    DB_COMERCIAL.INFO_SERVICIO_PROD_CARACT SPC,
    DB_COMERCIAL.ADMI_PRODUCTO_CARACTERISTICA PC,
    DB_COMERCIAL.ADMI_CARACTERISTICA AC
  WHERE S.ID_SERVICIO                 = SPC.SERVICIO_ID
  AND SPC.PRODUCTO_CARACTERISITICA_ID = PC.ID_PRODUCTO_CARACTERISITICA
  AND PC.CARACTERISTICA_ID            = AC.ID_CARACTERISTICA
  AND SPC.ESTADO                      = 'Activo'
  AND AC.DESCRIPCION_CARACTERISTICA   = 'TIPO LICENCIAMIENTO SERVICE'
  AND SPC.VALOR                       = 'Licencia  VEEAM  Replicacion   x  vm'
  );

UPDATE DB_COMERCIAL.INFO_SERVICIO_PROD_CARACT
SET VALOR                      = 'Ubuntu Server'
WHERE ID_SERVICIO_PROD_CARACT IN
  (SELECT SPC.ID_SERVICIO_PROD_CARACT
  FROM 
    DB_COMERCIAL.INFO_SERVICIO S,
    DB_COMERCIAL.INFO_SERVICIO_PROD_CARACT SPC,
    DB_COMERCIAL.ADMI_PRODUCTO_CARACTERISTICA PC,
    DB_COMERCIAL.ADMI_CARACTERISTICA AC
  WHERE S.ID_SERVICIO                 = SPC.SERVICIO_ID
  AND SPC.PRODUCTO_CARACTERISITICA_ID = PC.ID_PRODUCTO_CARACTERISITICA
  AND PC.CARACTERISTICA_ID            = AC.ID_CARACTERISTICA
  AND SPC.ESTADO                      = 'Activo'
  AND AC.DESCRIPCION_CARACTERISTICA   = 'TIPO LICENCIAMIENTO SERVICE'
  AND SPC.VALOR                       = 'Ubuntu Linux'
  );

