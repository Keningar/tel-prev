/*
  Privilegios para que el esquema 'DB_EXTERNO' puedan ejecutar los paquetes del esquema 'DB_COMERCIAL'.
*/
GRANT EXECUTE ON DB_COMERCIAL.CMKG_PROMOCIONES      TO DB_EXTERNO;
GRANT EXECUTE ON DB_COMERCIAL.CMKG_PROMOCIONES_UTIL TO DB_EXTERNO;
GRANT EXECUTE ON DB_GENERAL.GNRLPCK_UTIL            TO DB_EXTERNO;
GRANT SELECT  ON DB_GENERAL.INFO_ERROR              TO DB_EXTERNO,DB_SOPORTE;
GRANT EXECUTE ON DB_SOPORTE.SPKG_REPORTES           TO DB_COMERCIAL;
