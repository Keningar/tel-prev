INSERT INTO DB_SEGURIDAD.SEGU_PERFIL_PERSONA 
(
    PERFIL_ID,
    PERSONA_ID,
    OFICINA_ID,
    EMPRESA_ID,
    USR_CREACION,
    FE_CREACION,
    IP_CREACION
)
SELECT (SELECT ID_PERFIL FROM DB_SEGURIDAD.SIST_PERFIL WHERE NOMBRE_PERFIL = 'Tn Administrar Casos Todo') AS PERFIL_ID,
spp.persona_id,
spp.oficina_id,
spp.empresa_id ,
'telcosRegClon', 
SYSDATE, 
'172.0.0.1'
FROM  DB_SEGURIDAD.SEGU_PERFIL_PERSONA  SPP WHERE
SPP.PERFIL_ID = (SELECT ID_PERFIL FROM DB_SEGURIDAD.SIST_PERFIL WHERE NOMBRE_PERFIL = 'Administrar Casos Todo')
AND empresa_id = 10;

