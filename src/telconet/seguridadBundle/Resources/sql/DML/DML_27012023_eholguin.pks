SELECT (SELECT ID_PERFIL FROM DB_SEGURIDAD.SIST_PERFIL WHERE NOMBRE_PERFIL = 'Tn Perfil Coordinador') AS PERFIL_ID,
spp.persona_id,
spp.oficina_id,
spp.empresa_id ,
'telcosRegClon', 
SYSDATE, 
'172.0.0.1'
FROM  DB_SEGURIDAD.SEGU_PERFIL_PERSONA  SPP WHERE
SPP.PERFIL_ID = (SELECT ID_PERFIL FROM DB_SEGURIDAD.SIST_PERFIL WHERE NOMBRE_PERFIL = 'Perfil Coordinador MD')
AND spp.empresa_id = 10 AND spp.persona_id NOT IN (SELECT SPP2.PERSONA_ID FROM  DB_SEGURIDAD.SEGU_PERFIL_PERSONA SPP2
WHERE SPP2.PERFIL_ID = (SELECT ID_PERFIL FROM DB_SEGURIDAD.SIST_PERFIL WHERE NOMBRE_PERFIL = 'Tn Perfil Coordinador')

AND SPP2.PERSONA_ID  in(SELECT spp.persona_id
                    FROM  DB_SEGURIDAD.SEGU_PERFIL_PERSONA  SPP WHERE
                    SPP.PERFIL_ID = (SELECT ID_PERFIL FROM DB_SEGURIDAD.SIST_PERFIL WHERE NOMBRE_PERFIL = 'Perfil Coordinador MD')
                    AND empresa_id = 10)
AND SPP2.OFICINA_ID IN(SELECT spp.oficina_id
                    FROM  DB_SEGURIDAD.SEGU_PERFIL_PERSONA  SPP WHERE
                    SPP.PERFIL_ID = (SELECT ID_PERFIL FROM DB_SEGURIDAD.SIST_PERFIL WHERE NOMBRE_PERFIL = 'Perfil Coordinador MD')
                    AND empresa_id = 10)
AND SPP2.EMPRESA_ID = '10');



