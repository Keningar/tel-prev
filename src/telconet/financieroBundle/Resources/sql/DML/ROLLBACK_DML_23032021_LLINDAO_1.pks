-- REVERSO DE PARAMETRO CONTABILIZAR ANTC POR EMPRESA
DELETE DB_GENERAL.ADMI_PARAMETRO_DET APD
WHERE APD.DESCRIPCION = 'CONTABILIZA_ANTICIPO_NOTA_CREDITO'
AND APD.EMPRESA_COD = '10'
AND EXISTS (SELECT NULL
            FROM DB_GENERAL.ADMI_PARAMETRO_CAB APC
            WHERE APC.NOMBRE_PARAMETRO = 'VALIDACIONES_PROCESOS_CONTABLES'
            AND APC.ID_PARAMETRO = APD.PARAMETRO_ID)
;


