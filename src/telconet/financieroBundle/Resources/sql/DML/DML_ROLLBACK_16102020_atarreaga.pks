DELETE FROM DB_COMERCIAL.ADMI_TIPO_SOLICITUD WHERE DESCRIPCION_SOLICITUD = 'SOLICITUD FACTURACION POR REUBICACION';
DELETE FROM DB_COMERCIAL.ADMI_TIPO_SOLICITUD WHERE DESCRIPCION_SOLICITUD = 'SOLICITUD NOTA CREDITO POR REUBICACION';


DELETE FROM DB_GENERAL.ADMI_PARAMETRO_DET WHERE 
PARAMETRO_ID = (SELECT ID_PARAMETRO FROM DB_GENERAL.ADMI_PARAMETRO_CAB WHERE NOMBRE_PARAMETRO = 'FACTURACION_SOLICITUDES' AND ESTADO = 'Activo')
AND DESCRIPCION = 'Solicitud facturacion por reubicacion' AND VALOR1 = 'SOLICITUD FACTURACION POR REUBICACION';


DELETE FROM DB_COMERCIAL.ADMI_CARACTERISTICA WHERE DESCRIPCION_CARACTERISTICA = 'SOLICITUD_FACT_REUBICACION';
DELETE FROM DB_COMERCIAL.ADMI_CARACTERISTICA WHERE DESCRIPCION_CARACTERISTICA = 'SOLICITUD_NC_REUBICACION';
DELETE FROM DB_COMERCIAL.ADMI_CARACTERISTICA WHERE DESCRIPCION_CARACTERISTICA = 'NUMERO_TAREA_REUBICACION';
DELETE FROM DB_COMERCIAL.ADMI_CARACTERISTICA WHERE DESCRIPCION_CARACTERISTICA = 'OBSERVACION_NC_REUBICACION';
DELETE FROM DB_COMERCIAL.ADMI_CARACTERISTICA WHERE DESCRIPCION_CARACTERISTICA = 'AUTORIZADO_NC_REUBICACION';
DELETE FROM DB_COMERCIAL.ADMI_CARACTERISTICA WHERE DESCRIPCION_CARACTERISTICA = 'PORCENTAJE_NC_REUBICACION';
DELETE FROM DB_COMERCIAL.ADMI_CARACTERISTICA WHERE DESCRIPCION_CARACTERISTICA = 'MOTIVO_NC_REUBICACION';


DELETE FROM DB_GENERAL.ADMI_PARAMETRO_DET 
WHERE PARAMETRO_ID = (SELECT ID_PARAMETRO 
                      FROM DB_GENERAL.ADMI_PARAMETRO_CAB  
                      WHERE NOMBRE_PARAMETRO = 'PROCESO_REUBICACION' 
                      AND MODULO = 'FINANCIERO');
