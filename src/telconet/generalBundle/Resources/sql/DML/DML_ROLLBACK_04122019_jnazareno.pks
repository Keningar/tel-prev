--ELIMINAMOS PORCENTAJE DE FOTOS OBLIGATORIAS
DELETE FROM DB_GENERAL.ADMI_PARAMETRO_DET DETALLE
WHERE
    PARAMETRO_ID = (
        SELECT
            ID_PARAMETRO
        FROM
            DB_GENERAL.ADMI_PARAMETRO_CAB
        WHERE
            NOMBRE_PARAMETRO = 'BANDERA_AUDITORIA_COORDINADOR'
    );

--ELIMINAMOS CABECERA DE PORCENTAJE DE FOTOS OBLIGATORIAS
DELETE FROM DB_GENERAL.ADMI_PARAMETRO_CAB CABECERA
WHERE
    CABECERA.NOMBRE_PARAMETRO = 'BANDERA_AUDITORIA_COORDINADOR';

COMMIT;
/
