--ELIMINANDO MODELO FORTIGATE PARA INSTALACION SM TELLION	
DELETE 
FROM 
DB_GENERAL.ADMI_PARAMETRO_DET 
WHERE PARAMETRO_ID = 
(SELECT ID_PARAMETRO FROM ADMI_PARAMETRO_CAB WHERE NOMBRE_PARAMETRO = 'MODELOS_CPE_WIFI_ACTIVACION_SB_TELLION')
AND USR_CREACION = 'rmoranc' 
AND VALOR2 = 'FG-30E';

--ELIMINANDO FORMATOS DE MAC FORTIGATE PARA INSTALACION SM TELLION
DELETE 
FROM 
DB_GENERAL.ADMI_PARAMETRO_DET 
WHERE PARAMETRO_ID = 
(SELECT ID_PARAMETRO FROM ADMI_PARAMETRO_CAB WHERE NOMBRE_PARAMETRO = 'OUI_MAC_CPE_WIFI_ACTIVACION_SB_TELLION')
AND USR_CREACION = 'rmoranc' 
AND VALOR1 = 'FG-30E';


--ELIMINANDO ELEMENTO FORTIGATE
DELETE 
FROM 
DB_INFRAESTRUCTURA.ADMI_TIPO_ELEMENTO 
WHERE 
DESCRIPCION_TIPO_ELEMENTO = 'FORTIGATE 30E' 
AND USR_CREACION = 'rmoranc';

COMMIT;