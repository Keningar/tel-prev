--ELIMINAMOS DETALLE DE EQUIPOS UTILIZADOS EN INSTALACIONES TN 
DELETE 
FROM 
DB_GENERAL.ADMI_PARAMETRO_DET 
WHERE
USR_CREACION = 'rmoranc' 
AND DESCRIPCION = 'EQUIPOS EN PRODUCTO TN';


--ELIMINANDO PROGRESOS DE INSTALACION TN FTTX
DELETE 
FROM 
DB_SOPORTE.INFO_PROGRESO_PORCENTAJE
WHERE
TAREA_ID =  5151 
AND EMPRESA_ID = 10;	
	

