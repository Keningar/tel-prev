/**
 * DML PARA REVERSAR LOS REGISTROS INGRESADOS.
 */

SELECT ID_PLANTILLA
  INTO Ln_IdPlantilla
  FROM DB_COMUNICACION.ADMI_PLANTILLA
  WHERE CODIGO='SAESUBIDA';
DELETE FROM DB_COMUNICACION.INFO_ALIAS_PLANTILLA
	WHERE PLANTILLA_ID = Ln_IdPlantilla;

DELETE FROM DB_DB_COMUNICACION.ADMI_PLANTILLA WHERE CODIGO = 'SAESUBIDA'
 AND MODULO = 'TECNICO';

COMMIT;

/

