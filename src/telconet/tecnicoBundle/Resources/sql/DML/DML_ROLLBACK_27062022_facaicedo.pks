
-- REVERSO DE LA CABECERA DE PARAMETROS DE 'PARAMETROS_CREACION_MONITOREO_TELCOGRAPH'
DELETE DB_GENERAL.ADMI_PARAMETRO_DET
WHERE
    PARAMETRO_ID = (
        SELECT
            ID_PARAMETRO
        FROM
            DB_GENERAL.ADMI_PARAMETRO_CAB
        WHERE
            NOMBRE_PARAMETRO = 'PARAMETROS_CREACION_MONITOREO_TELCOGRAPH'
    );
DELETE DB_GENERAL.ADMI_PARAMETRO_CAB
WHERE
    NOMBRE_PARAMETRO = 'PARAMETROS_CREACION_MONITOREO_TELCOGRAPH';

COMMIT;
/
