-- Reverso de la caracteristica para la fecha de fin de la ejecución programada
-- Deshabilitar el control del BW de la interface
--=======================================================================

-- REVERSO DE LOS DETALLES DE PARAMETROS DE 'TIPO_VALIDACION_CONTROL_BW_INTERFACE'
DELETE DB_GENERAL.ADMI_PARAMETRO_DET
WHERE
    PARAMETRO_ID = (
        SELECT
            ID_PARAMETRO
        FROM
            DB_GENERAL.ADMI_PARAMETRO_CAB
        WHERE
            NOMBRE_PARAMETRO = 'TIPO_VALIDACION_CONTROL_BW_INTERFACE'
    );
-- REVERSO DE LA CABECERA DE PARAMETROS DE 'TIPO_VALIDACION_CONTROL_BW_INTERFACE'
DELETE DB_GENERAL.ADMI_PARAMETRO_CAB
WHERE
    NOMBRE_PARAMETRO = 'TIPO_VALIDACION_CONTROL_BW_INTERFACE';

