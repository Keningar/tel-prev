            <tr>
                <td width="50%" align="center" valign="top">
                    <p align="justify">
                        <strong>{{informacionContrato.nombresContacto}}</strong><br />
                        <strong> C.C.  #  {{informacionContrato.cedContactoNodo}}</strong><br />
                        <strong> Arrendador{{informacionContrato.articuloFin}}</strong><br />
                        <strong>{{informacionContrato.infoContactoRazones}}</strong>
                    </p>
               </td>

                <td align="center" valign="top"><p align="justify">
                        <strong>p.  Telconet S.A.</strong><br />
                        <strong>R.U.C. # 0991327371001</strong> <br />
                        <strong>Arrendataria</strong><br />
                    </p>
                    <p align="justify">&nbsp;</p>
                    <p align="justify"><br />
                        <strong>{{informacionContrato.repLegal}}</strong><br />
                        <strong> C.C. No. {{informacionContrato.cedulaRepLegal}}</strong><br />
                        <strong> Apoderado Especial </strong></p>
                </td>
            </tr>
        </table>  
      </div>
